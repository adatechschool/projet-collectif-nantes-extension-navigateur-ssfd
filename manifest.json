{
    "manifest_version": 3,
    "name": "grumpyCat",
    "version": "1.0",
    "description": "grumpy Cat everywhere",
    "icons": {
      "64": "photo/grumpyCat.png"
    },
   "action": {
    
    "default_icon": {
      "64":"photo/grumpyCat.png" 
    }
},
"content_scripts":[
{
  "js":["content.js"],
  "matches": ["<all_urls>"],
  "run_at": "document_end"
}

],
    "permissions": ["scripting", "activeTab"]
}

