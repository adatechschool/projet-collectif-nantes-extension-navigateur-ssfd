{
    "manifest_version": 3,
    "name": "grumpAnimals",
    "version": "1.0",
    "description": "grumpy animals everywhere",
    "icons": {
      "64": "grumpyCat.png"
    },
   "action": {
    "default_popup":"popup.html",
    "default_icon": {
      "64":"grumpyCat.png" 

    }
},
    "permissions": ["scripting", "activeTab"],
    
"web_accessible_resources": [
    {
      "resources": ["grumpyCat.png","doogy.png","squirrel.png"],
      "matches": ["<all_urls>"]
    }
  ]
}

